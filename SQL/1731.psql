SELECT
    e.employee_id,
    e.name,
    r.reports_count,
    r.average_age
FROM Employees e
INNER JOIN (
    SELECT DISTINCT
        reports_to,
        COUNT(*) AS reports_count,
        ROUND(AVG(age)) AS average_age
    FROM Employees
    WHERE reports_to IS NOT NULL
    GROUP BY reports_to
    HAVING COUNT(*) >= 1
) r
    ON r.reports_to = e.employee_id
ORDER BY employee_id
